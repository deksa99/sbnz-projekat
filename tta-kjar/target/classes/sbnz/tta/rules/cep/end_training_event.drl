package sbnz.tta.rules.cep;

import com.tta.app.model.events.EndTrainingEvent;

rule "Halt drools when training is over"
    when
       	$endTr: EndTrainingEvent(processed == false)
    then
    	modify ($endTr) {
    	setProcessed(true);
    	}
    	System.out.println("Trening je zavr≈°en");
    	drools.halt();
end